generator client {
  provider = "prisma-client-js"
  output   = "./prisma-client"
}

datasource db {
  provider = "sqlite"
  url      = "file:../db.db"
}

model prs {
  id             String    @id
  authorName     String
  authorEmail    String
  base_branch    String
  compare_branch String
  status         Int
  name           String
  description    String
  createdAt      DateTime? @default(now())
  mergedAt       DateTime?
}
